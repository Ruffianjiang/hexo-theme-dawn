<nav class="pagination is-centered is-small" role="navigation" aria-label="pagination">
    <% if (page.prev != 0) { %>
        <a class="pagination-previous" href="<%= url_for(page.prev_link) %>" title="<%= __('pagination.prev') %>">Previous</a>
    <% } else { %>
        <div></div>
    <% } %>

    <% if (page.next != 0) { %>
        <a class="pagination-next" href="<%= url_for(page.next_link) %>" title="<%= __('pagination.next') %>">Next
            page</a>
    <% } else { %>
        <div></div>
    <% } %>

    <% if (page.prev != 0) { %>
        <ul class="pagination-list">
            <%- paginator({
                // prev_text: '<i class="icon icon-angle-left"></i> ' + __('pagination.prev'),
                // next_text: __('pagination.next') + ' <i class="icon icon-angle-right"></i>',
                prev_next: false,
                escape: false
            }) %>
        </ul>
    <% } %>
</nav>




